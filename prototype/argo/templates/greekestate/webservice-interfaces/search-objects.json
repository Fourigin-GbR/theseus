{
	"path": "/objects/sale/object_*",
	"query": {
		"metaData:/attributes/region EQUAL": "loutraki",
		"metaData:/attributes/type NOT_CONTAINS": "apartment",
		"metaData:/attributes/type CONTAINS": "house",
		"content:/price LESS": "22500"
	},
	"sorting": {
		"value": "content:/price LESS",
		"type": "ascending"
	}
}


{
  "region": ["loutraki"],
  "filter": [
    {
      "apartment": false,
      "house": true,
      "priceMax": 22500
    }
  ],
  "sorting": "priceUp"
}