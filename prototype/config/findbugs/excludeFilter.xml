<FindBugsFilter>
    <Match>
        <Source name="~.*\.groovy" />
        <Bug pattern="SE_NO_SERIALVERSIONID"/>
    </Match>
    <!-- Crash commands -->
    <Match>
        <Package name="commands"/>
    </Match>

    <!-- config classes -->
    <Match>
        <Class name="~.*\.*Configuration"/>
        <Bug pattern="DMI_HARDCODED_ABSOLUTE_FILENAME"/>
    </Match>

</FindBugsFilter>